    document.getElementById('send-reset-link').addEventListener('click', async function() {
    const email = document.getElementById('reset-email').value;
    const resetMessage = document.getElementById('reset-message');
    
    if (!email) {
      resetMessage.textContent = "Por favor ingresa tu correo electr√≥nico";
      resetMessage.classList.remove('hidden');
      resetMessage.classList.add('text-red-500');
      resetMessage.classList.remove('text-green-500');
      return;
    }

    try {
      // Mostrar mensaje de carga
      resetMessage.textContent = "Enviando enlace...";
      resetMessage.classList.remove('hidden');
      resetMessage.classList.remove('text-red-500');
      resetMessage.classList.add('text-blue-500');
      
      // Deshabilitar el bot√≥n para evitar m√∫ltiples clics
      this.disabled = true;
      this.textContent = "Enviando...";
      
      // Enviar la solicitud al servidor
      const response = await fetch('/request-password-reset', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify({ email: email })
      });
      
      const data = await response.json();
      
      if (response.ok) {
        resetMessage.textContent = "‚úîÔ∏è Se ha enviado un enlace a tu correo electr√≥nico. Revisa tu bandeja de entrada.";
        resetMessage.classList.remove('text-blue-500');
        resetMessage.classList.add('text-green-500');
      } else {
        resetMessage.textContent = data.message || "‚ùå Error al enviar el enlace. Intenta nuevamente.";
        resetMessage.classList.remove('text-blue-500');
        resetMessage.classList.add('text-red-500');
      }
    } catch (error) {
      console.error("Error:", error);
      resetMessage.textContent = "‚ùå Error de conexi√≥n. Intenta nuevamente.";
      resetMessage.classList.remove('text-blue-500');
      resetMessage.classList.add('text-red-500');
    } finally {
      // Restaurar el bot√≥n
      this.disabled = false;
      this.textContent = "Enviar Enlace";
      
      // Ocultar el mensaje despu√©s de 5 segundos
      setTimeout(() => {
        resetMessage.classList.add('hidden');
      }, 5000);
    }
  });

</script>

